<%- include('partials/init-doc.ejs', {tituloPagina: 'Veterinarios'}) %> 

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        <%- include('partials/spinner.ejs') %> 
        <!-- Spinner End -->

        <!-- Header Start -->
        <%- include('partials/main-header.ejs', {active: 6, usuario: usuario}) %> 
        <!-- Header End -->


        <!-- Team Start -->
        <div class="container-xxl py-5">
            <div class="container">
                <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                    <h6 class="section-title text-center text-info text-uppercase">PetCare</h6>
                    <h1 class="mb-5">Veterinarios <span class="text-info text-uppercase">Registrados</span></h1>
                </div>
                <div class="row g-4">
                    <div class="card">
                        <div class="card-title">
                            <h4>Usuarios | Veterinarios</h4>
                        </div>
                        <div class="bootstrap-data-table-panel">
                            <div class="table-responsive">
                                <table id="row-select" class="display table table-borderd table-hover">
                                    <thead>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Cedula</th>
                                            <th>Celular</th>
                                            <th>Correo</th>
                                            <th>Opciones</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% veterinarios.forEach((veterinario)=>{ %> 
                                            <tr>
                                                <td><%= veterinario.nombre %> <%= veterinario.apellido %></td>
                                                <td><%= veterinario.cedula %></td>
                                                <td><%= veterinario.celular %></td>
                                                <td><%= veterinario.correo %></td>
                                                <td>
                                                    <div class="button-list">
                                                        <div class="btn-group btn-group-justified">
                                                            <a href="/perfilveterinario?veterinario=<%= veterinario.id %>" class="btn btn-info btn-sm" target="_self" title="Ver Perfil de Pet Owner"><i class="fa fa-user"></i></a>
                                                        </div>
                                                    </div></td>
                                            </tr>
                                        <% }) %> 
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Cedula</th>
                                            <th>Celular</th>
                                            <th>Correo</th>
                                            <th>Opciones</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Team End -->


        <!-- Footer Start -->
        
        <div class="copyright">
            <div class="row">
                <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                    &copy; <a class="border-bottom" href="#">PetCare</a>, Todos los derechos reservados. 
					<!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
					Designed By <a class="border-bottom" href="https://htmlcodex.com">HTML Codex</a>
                    <br>Distributed By: <a class="border-bottom" href="https://themewagon.com" target="_blank">ThemeWagon</a>
                </div>
            </div>
        </div>
           
        <!-- Footer End -->

        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <%- include('partials/linksjs.ejs') %> 
    <%- include('partials/cierrebody.ejs') %> 